@article{zheng_computation_2015,
	title = {Computation of the {Correlated} {Metal}-{Insulator} {Transition} in {Vanadium} {Dioxide} from {First} {Principles}},
	volume = {114},
	url = {http://link.aps.org/doi/10.1103/PhysRevLett.114.176401},
	doi = {10.1103/PhysRevLett.114.176401},
	abstract = {Vanadium dioxide (VO2) is a paradigmatic example of a strongly correlated system that undergoes a metal-insulator transition at a structural phase transition. To date, this transition has necessitated significant post hoc adjustments to theory in order to be described properly. Here we report standard state-of-the-art first principles quantum Monte Carlo (QMC) calculations of the structural dependence of the properties of VO2. Using this technique, we simulate the interactions between electrons explicitly, which allows for the metal-insulator transition to naturally emerge, importantly without ad hoc adjustments. The QMC calculations show that the structural transition directly causes the metal-insulator transition and a change in the coupling of vanadium spins. This change in the spin coupling results in a prediction of a momentum-independent magnetic excitation in the insulating state. While two-body correlations are important to set the stage for this transition, they do not change significantly when VO2 becomes an insulator. These results show that it is now possible to account for electron correlations in a quantitatively accurate way that is also specific to materials.},
	number = {17},
	urldate = {2015-05-08},
	journal = {Phys. Rev. Lett.},
	author = {Zheng, Huihuo and Wagner, Lucas K.},
	month = apr,
	year = {2015},
	pages = {176401},
	file = {APS Snapshot:/Users/lkwagner/Library/Application Support/Firefox/Profiles/cfsyd5xw.default/zotero/storage/Q43FMA7U/PhysRevLett.114.html:text/html;Zheng_Wagner_2015_Computation of the Correlated Metal-Insulator Transition in Vanadium Dioxide.pdf:/Users/lkwagner/Library/Application Support/Firefox/Profiles/cfsyd5xw.default/zotero/storage/ZRB5P4Q3/Zheng_Wagner_2015_Computation of the Correlated Metal-Insulator Transition in Vanadium Dioxide.pdf:application/pdf}
}

@article{wu_interaction_2015,
	title = {The interaction between hexagonal boron nitride and water from first principles},
	volume = {142},
	issn = {0021-9606, 1089-7690},
	url = {http://scitation.aip.org/content/aip/journal/jcp/142/23/10.1063/1.4922491},
	doi = {10.1063/1.4922491},
	abstract = {The use of hexagonal boron nitride (h-BN) in microfluidic and nanofluidic applications requires a fundamental understanding of the interaction between water and the h-BN surface. A crucial component of the interaction is the binding energy, which is sensitive to the treatment of electron correlation. In this work, we use state of the art quantum Monte Carlo and quantum chemistry techniques to compute the binding energy. Compared to high-level many-body theory, we found that the second-order M{\o}ller-Plesset perturbation theory captures the interaction accurately and can thus be used to develop force field parameters between h-BN and water for use in atomic scale simulations. On the contrary, density functional theory with standard dispersion corrections tends to overestimate the binding energy by approximately 75\%.},
	number = {23},
	urldate = {2015-06-22},
	journal = {The Journal of Chemical Physics},
	author = {Wu, Yanbin and Wagner, Lucas K. and Aluru, Narayana R.},
	month = jun,
	year = {2015},
	keywords = {Angular correlation, Carbon nanotubes, Density functional theory, Monolayers, Water energy interactions},
	pages = {234702},
	file = {Snapshot:/Users/lkwagner/Library/Application Support/Firefox/Profiles/cfsyd5xw.default/zotero/storage/9JX6HC6E/1.html:text/html;Wu et al_2015_The interaction between hexagonal boron nitride and water from first principles.pdf:/Users/lkwagner/Library/Application Support/Firefox/Profiles/cfsyd5xw.default/zotero/storage/4G8QR7RJ/Wu et al_2015_The interaction between hexagonal boron nitride and water from first principles.pdf:application/pdf}
}

@article{wagner_ground_2015,
	title = {Ground state of doped cuprates from first-principles quantum {Monte} {Carlo} calculations},
