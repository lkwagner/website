	pages = {045105},
	file = {APS Snapshot:/Users/lkwagner/Library/Application Support/Firefox/Profiles/cfsyd5xw.default/zotero/storage/6ZVHSDEC/PhysRevB.94.html:text/html;Narayan et al_2016_Computational and experimental investigation for new transition metal selenides.pdf:/Users/lkwagner/Library/Application Support/Firefox/Profiles/cfsyd5xw.default/zotero/storage/WGUEHTUG/Narayan et al_2016_Computational and experimental investigation for new transition metal selenides.pdf:application/pdf}
}

